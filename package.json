{
  "name": "pwa-mgt-disaster-management",
  "version": "0.0.1",
  "description": "A demo to show case PWA builder and Microsoft Graph Toolkit!",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist/ && tsc && rollup -c",
    "build:watch": "tsc --watch",
    "build:dev": "tsc",
    "start": "concurrently \"tsc --watch\" \"es-dev-server --app-index index.html --compatibility none --node-resolve --watch --open\"",
    "dev": "concurrently \"tsc --watch\" \"es-dev-server --app-index index.html --compatibility none --node-resolve --watch --open\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@microsoft/mgt": "^2.0.1",
    "@pwabuilder/pwainstall": "^1.6.7",
    "@vaadin/router": "^1.7.2",
    "lit-element": "^2.4.0"
  },
  "devDependencies": {
    "@microsoft/microsoft-graph-types": "^1.26.0",
    "@open-wc/rollup-plugin-html": "^1.2.5",
    "@rollup/plugin-strip": "^1.3.3",
    "concurrently": "^5.3.0",
    "es-dev-server": "^1.57.8",
    "lit-analyzer": "^1.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.33.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.1",
    "rollup-plugin-workbox": "^5.2.1",
    "typescript": "^3.9.7"
  }
